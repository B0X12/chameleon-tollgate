<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/history_item_layout"
    android:layout_height="67dp"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/tollgate_button_press">

    <ImageView
        android:id="@+id/auth_ico"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="25dp"
        android:adjustViewBounds="true"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.481" />

    <TextView
        android:id="@+id/auth_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginStart="14dp"
        android:text="TextView"
        android:fontFamily="@font/notosans_bold"
        android:includeFontPadding="false"
        app:layout_constraintStart_toEndOf="@+id/auth_ico"
        app:layout_constraintTop_toTopOf="@+id/auth_ico" />

    <TextView
        android:id="@+id/auth_subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="TextView"
        android:textSize="10dp"
        android:fontFamily="@font/notosans_medium"
        android:includeFontPadding="false"
        app:layout_constraintStart_toStartOf="@+id/auth_title"
        app:layout_constraintTop_toBottomOf="@+id/auth_title" />

    <TextView
        android:id="@+id/auth_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/notosans_regular"
        android:includeFontPadding="false"
        android:text="TextView"
        android:textSize="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/auth_subtitle"
        app:layout_constraintEnd_toStartOf="@+id/auth_result"
        app:layout_constraintStart_toEndOf="@+id/auth_subtitle" />

    <ImageView
        android:id="@+id/auth_result"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_marginEnd="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.488" />
</androidx.constraintlayout.widget.ConstraintLayout>